                    </div>
                    <div class="user-box">
                        <input type="password" name="password" required="">
                        <label>Password</label>
                    </div>
                      
                        {{>message}}
                   
        
                    <div class="button-form">
        
                        <button type="submit" id="submit" >
                           Submit
                      </button>


        
                        <div id="register">
                             Don't have an account ?
                            <a href="/contact">
                                Register
                            </a>
                        </div>
        
                    </div>
        
                </form>
            </div>

</section>












