        <section class="header">
         

          <h1>Edit Your Profile</h1>
          <form class="row g-3" action="/profile" method="POST">
  <div class="col-md-6">
    <label for="inputEmail4" class="form-label">Name</label>
    <input type="text" class="form-control" id="inputEmail4" name="name" value="{{name}}">
  </div>
  <div class="col-md-6">
    <label for="inputPassword4" class="form-label">Password</label>
    <input type="password" class="form-control" id="inputPassword4" name="password" value="{{password}}">
  </div>
  <div class="col-12">
    <label for="inputAddress" class="form-label">Email</label>
    <input type="email" class="form-control" id="inputAddress" placeholder="44/5" name="email" value="{{email}}">
  </div>
    <div class="col-12">
    <label for="inputAddress" class="form-label">Phone</label>
    <input type="tel" class="form-control" id="inputAddress" placeholder="44/5" max="10" name="phone" value="{{phone}}">
  </div>
 

  
  <div class="col-12">
    <button type="submit" class="btn btn-primary">Update</button>
  </div>
</form>

